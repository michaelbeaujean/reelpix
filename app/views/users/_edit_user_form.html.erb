<%= form_for(@user) do |f| %>
  <%= f.label(:name) %> <br>
  <%= f.text_field(:name) %> <br>
  <%= f.label(:username) %> <br>
  <%= f.text_field(:username) %> <br>
  <%= f.label(:password) %> <br>
  <%= f.password_field(:password) %> <br>
  <%= f.label(:password_confirmation) %> <br>
  <%= f.password_field(:password_confirmation) %> <br>
  <%= f.label(:image_url) %> <br>
  <%= f.text_field(:image_url) %> <br>
  <%= f.label(:location) %> <br>
  <%= f.text_field(:location) %> <br>
  <%= f.label(:age) %> <br>
  <%= f.number_field(:age) %> <br>
  <%= f.label(:favorite_movies) %> <br>
  <%= f.text_field(:favorite_movies) %> <br>
  <%= f.label(:favorite_tv_shows) %> <br>
  <%= f.text_field(:favorite_tv_shows) %> <br>
  <%= f.submit %>
<% end %>
